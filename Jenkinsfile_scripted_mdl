#!groovy
# Scripted Pipeline
node {
   stage('Git Checkout') {
        checkout scm
   }
   stage('Build Simulink Model') {
        try {
            bat('example_build.bat')
        } finally {
            archive('*.log','*_ert_rtw/*')
        }
   }
   stage('Run dSpace Tests') {
        echo('Run dSpace')
   }
}